<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.roghaari"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name="com.roghaari.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.roghaari.DashboardActivity"
            android:label="@string/title_activity_dashboard" />
        <activity android:name="com.roghaari.BaseActivity" />
        <activity android:name="com.roghaari.TabBarActivity" />
        <activity android:name="com.roghaari.DashboardFragment" />
        <activity android:name="com.roghaari.HistoryFragment" />
        <activity android:name="com.roghaari.NotificationFragment"></activity>
    </application>

</manifest>